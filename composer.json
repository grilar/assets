{
    "name": "grilar/assets",
    "description": "Laravel assets management",
    "license": "MIT",
    "keywords": [
        "grilar",
        "grilar cms",
        "grilar platform",
        "grilar assets"
    ],
    "authors": [
        {
            "name": "Grilar"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0|^8.1",
        "laravel/framework": "^5.6|^6|^7|^8|^9|^10"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0|^8.0|^9.0|^10.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Grilar\\Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Grilar\\Assets\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Grilar\\Assets\\Providers\\AssetsServiceProvider"
            ],
            "aliases": {
                "Assets": "Grilar\\Assets\\Facades\\AssetsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
